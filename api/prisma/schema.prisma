datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int           @id @default(autoincrement())
  firstName   String
  lastName    String
  email       String        @unique
  reservation Reservation[]
}

model Reservation {
  id     Int    @id @default(autoincrement())
  date   String
  time   String
  user   User   @relation(fields: [userId], references: [id])
  userId Int
}
